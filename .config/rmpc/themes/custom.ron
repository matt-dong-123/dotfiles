                default: (kind: Property(Filename))
            ),
            width: "50%",
        ),
        (
            prop: (kind: Property(Artist),
                default: (kind: Text("Unknown Artist"))
            ),
            width: "45%",
        ),
        (
            prop: (kind: Property(Duration)),
            width: "10%",
            alignment: Right,
        ),
    ],
    header: (
        rows: [
            (
                left: [
                    (kind: Property(Status(StateV2(playing_label: " üîä", paused_label: " ‚ùö‚ùö", stopped_label: " ‚èπÔ∏è"))), style: (fg: "yellow", modifiers: "Bold")),
                ],
                center: [
                    (kind: Property(Song(Title)), style: (modifiers: "Bold"),
                        default: (kind: Property(Song(Filename)), style: (modifiers: "Bold"))
                    )
                ],
                right: [
                    (kind: Text("Vol: "), style: (fg: "blue", modifiers: "Bold")),
                    (kind: Property(Status(Volume)), style: (fg: "blue", modifiers: "Bold")),
                    (kind: Text("% "), style: (fg: "blue", modifiers: "Bold"))
                ]
            ),
            (
                left: [
