# Apps
ctrl + shift + cmd - a : open -a "Zen"
ctrl + shift + cmd - d : open -a "Dorico 5"
ctrl + shift + cmd - f : open -a "Finder"
ctrl + shift + cmd - g : open -a "Clash Verge"
ctrl + shift + cmd - p : open -a "Passwords"
ctrl + shift + cmd - s : open -a "System Settings"
ctrl + shift + cmd - w : open -a "WeChat"
ctrl + shift + cmd - return : open -a "Ghostty"

ctrl + shift + cmd - i : kitten quick-access-terminal --instance-group=install ~/.config/omacase/bin/homebrew-install
ctrl + shift + cmd - u : kitten quick-access-terminal --instance-group=uninstall ~/.config/omacase/bin/homebrew-uninstall
ctrl + shift + cmd - t : kitten quick-access-terminal --instance-group=theme ~/.config/omacase/bin/theme-select
ctrl + shift + cmd - b : kitten quick-access-terminal --instance-group=bg ~/.config/omacase/bin/background-select
ctrl + shift + cmd - h : kitten quick-access-terminal --instance-group=tl ~/.config/bin/tl
ctrl + shift + cmd - q : kitten quick-access-terminal --instance-group=music sesh connect Music
shift - space : rmpc togglepause
alt + cmd - space : kitten quick-access-terminal ~/.config/omacase/bin/actions-select

:: leader @ : sketchybar --trigger skhd_mode_change SKHD_MODE_LABEL='󰠠 '
:: internet @ : sketchybar --trigger skhd_mode_change SKHD_MODE_LABEL='󰠠 i'
:: default : sketchybar --trigger skhd_mode_change SKHD_MODE_LABEL=' '

f13 ; leader

leader < escape ; default
leader < f13 ; default
leader < p ; default : open -a Preview
leader < v ; default : open -a VoiceMemos
leader < c ; default : open -a Chromium
leader < k ; default : open -a Karabiner-Elements

leader < i ; internet

internet < escape ; default
internet < f13 ; default
internet < g ; default : open "https://github.com"
internet < y ; default : open "https://youtu.be"
internet < r ; default : open "https://reddit.com"
internet < 0x2b ; default : open "https://github.com/matt-dong-123/dotfiles" # Comma
