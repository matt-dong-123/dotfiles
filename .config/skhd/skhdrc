# Apps
ctrl + shift + cmd - a : open -a "Zen"
ctrl + shift + cmd - d : open -a "Dorico 5"
ctrl + shift + cmd - f : open -a "Finder"
ctrl + shift + cmd - l : open -a "Logic Pro"
ctrl + shift + cmd - m : open -a "MuseScore 4"
ctrl + shift + cmd - p : open -a "Passwords"
ctrl + shift + cmd - s : open -a "System Settings"
ctrl + shift + cmd - w : open -a "WeChat"
ctrl + shift + cmd - z : open -a "zoom.us"
ctrl + shift + cmd - return : open -a "Wezterm"

ctrl + shift + cmd - i : alacritty -e ~/scripts/homebrew-install.sh
ctrl + shift + cmd - u : alacritty -e ~/scripts/homebrew-uninstall.sh
