# Apps
ctrl + shift + cmd - a : open -a "Zen"
ctrl + shift + cmd - d : open -a "Dorico 5"
ctrl + shift + cmd - f : open -a "Finder"
ctrl + shift + cmd - l : open -a "Logic Pro"
ctrl + shift + cmd - m : open -a "MuseScore 4"
ctrl + shift + cmd - p : open -a "Preview"
ctrl + shift + cmd - s : open -a "System Settings"
ctrl + shift + cmd - w : open -a "WeChat"
ctrl + shift + cmd - v : open -a "VoiceMemos"
ctrl + shift + cmd - z : open -a "zoom.us"
ctrl + shift + cmd - return : open -a "Wezterm"

ctrl + shift + cmd - i : alacritty -e ~/.config/omacase/bin/homebrew-install
ctrl + shift + cmd - u : alacritty -e ~/.config/omacase/bin/homebrew-uninstall
ctrl + shift + cmd - t : alacritty -e ~/.config/omacase/bin/theme-select
ctrl + shift + cmd - b : alacritty -e ~/.config/omacase/bin/background-select
alt + cmd - space : alacritty -e ~/.config/omacase/bin/actions-select
