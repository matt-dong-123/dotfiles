# Apps
ctrl + shift + cmd - a : open -a "Zen"
ctrl + shift + cmd - d : open -a "Dorico 5"
ctrl + shift + cmd - f : open -a "Finder"
ctrl + shift + cmd - g : open -a "Clash Verge"
ctrl + shift + cmd - l : open -a "Logic Pro"
ctrl + shift + cmd - m : open -a "MuseScore 4"
ctrl + shift + cmd - p : open -a "Passwords"
ctrl + shift + cmd - s : open -a "System Settings"
ctrl + shift + cmd - w : open -a "WeChat"
ctrl + shift + cmd - z : open -a "zoom.us"
ctrl + shift + cmd - return : open -a "Wezterm"

ctrl + shift + cmd - i : kitten quick-access-terminal ~/.config/omacase/bin/homebrew-install
ctrl + shift + cmd - u : kitten quick-access-terminal ~/.config/omacase/bin/homebrew-uninstall
ctrl + shift + cmd - t : kitten quick-access-terminal ~/.config/omacase/bin/theme-select
ctrl + shift + cmd - b : kitten quick-access-terminal ~/.config/omacase/bin/background-select
alt + cmd - space : kitten quick-access-terminal ~/.config/omacase/bin/actions-select
